# Optional: comma-separated Socrata domains to preconfigure (any domain is allowed at call time)
SODA_DOMAINS=data.cityofnewyork.us

# Optional: application token applied to all domains (recommended to avoid throttling)
SODA_APP_TOKEN=your_app_token_here

# Optional: per-domain rate cap (requests per hour)
SODA_REQUESTS_PER_HOUR=800
